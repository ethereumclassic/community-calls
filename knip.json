{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/pages/**/*.{astro,ts}",
    "src/content.config.ts",
    "astro.config.mjs",
    "netlify/functions/*.ts"
  ],
  "project": ["src/**/*.{astro,ts,tsx}", "netlify/**/*.ts"],
  "ignore": ["**/*.d.ts"],
  "ignoreDependencies": [
    "tailwindcss",
    "daisyui",
    "@tailwindcss/typography",
    "@iconify-json/lucide",
    "@iconify-json/simple-icons"
  ]
}
